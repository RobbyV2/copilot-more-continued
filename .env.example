# Copy this file to .env and replace the value with your GitHub Copilot refresh token
# Required settings
REFRESH_TOKEN=gho_some_token

# Request settings
MAX_TOKENS=10240
TIMEOUT_SECONDS=300
EDITOR_VERSION=vscode/1.97.2

# Rate limiting settings
SLEEP_BETWEEN_CALLS=0

# API security settings
# Optional comma-delimited list of valid API keys. If set, requests must include a valid key.
# Example: API_KEYS=sk-xxx,key1,pass
API_KEYS=

# Debug settings
RECORD_TRAFFIC=false
LOGURU_LEVEL=INFO